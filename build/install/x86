#!/bin/sh

CURDIR=`pwd`
TARGETDIR=${CURDIR}/build/target/x86

rm -fr ${TARGETDIR}
mkdir -p ${TARGETDIR}

cp -frvL build/platform/linux/libhilda.so ${TARGETDIR}
cp -frvL build/platform/linux/hilda.a ${TARGETDIR}

cp -frvL ${HILDA_LIB}/x86/libhilda.so ${TARGETDIR}
cp -frvL ${HILDA_LIB}/x86/hilda.a ${TARGETDIR}
